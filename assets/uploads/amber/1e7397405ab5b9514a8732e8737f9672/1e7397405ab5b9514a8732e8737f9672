window.modules["1"] = [function(require,module,exports){"use strict";
/* globals amplitude: false */

/* globals document: false */

/**
 * An API wrapper for our various analytics services
 *
 */

var domEventData;

var getDomEventData = function getDomEventData(el) {
  if (domEventData) return domEventData;
  el = el || document.querySelectorAll("script.js-analytics-js-data")[0];
  domEventData = JSON.parse(el.textContent);
  return domEventData;
}; // See https://segment.com/docs/sources/website/analytics.js/#track
// analytics.track(event, [properties], [options], [callback]);

/**
 * This function wraps the call to amplitude's logEvent method, but always looks for and adds the data in
 */


var track = function track(event, properties, callback) {
  var data = getDomEventData();
  var props = {};
  Object.assign(props, data, properties);
  amplitude.getInstance().logEvent(event, props, callback);
}; // Page is a special event that does its own properties


var page = function page(properties, callback) {
  amplitude.getInstance().logEvent("Loaded a Page", properties, callback);
};

module.exports.getDomEventData = getDomEventData;
module.exports.page = page;
module.exports.track = track;
}, {}];
window.modules["2"] = [function(require,module,exports){/* globals document: false */
"use strict";

function isAbEnabled() {
  /**
   * Create a temporary "ad" node for adblockers to target
   */
  var tempAd = document.createElement("div");
  tempAd.innerHTML = "&nbsp";
  tempAd.className = "ad-container";
  document.body.appendChild(tempAd);
  var enabled = tempAd.offsetHeight === 0;
  document.body.removeChild(tempAd);
  return enabled;
}

module.exports = {
  isAbEnabled: isAbEnabled
};
}, {}];
window.modules["3"] = [function(require,module,exports){"use strict"; // get Slate Plus (splus) cookie

var cookies = require(49),
    splusCookieJson = cookies.getJSON("splus");
/**
 * Return if Member
 * @returns {boolean}
 */


function isMember() {
  var isMember = false;

  if (splusCookieJson) {
    isMember = true;
  }

  return isMember;
}
/**
 * Return Member Type
 * @returns {string}
 */


function getMembershipType() {
  var membershipType = "none";

  if (splusCookieJson) {
    membershipType = splusCookieJson.membershipType;
  }

  return membershipType;
}
/**
 * Return Member Info
 * @returns {json}
 */


function getMembershipInfo() {
  var myslateUrl = document.documentElement.dataset.myslateurl;
  var url = myslateUrl + "/users/slate_page_info/";
  var options = {
    method: "GET",
    credentials: "include",
    //pass cookies
    headers: {
      Accept: "application/json"
    }
  };

  if (splusCookieJson) {
    //Returns a promise. To get the value, use `then` in component's js
    return fetch(url, options).then(function (response) {
      return response.json();
    }).catch(function (error) {
      console.log("Fetch for myslate membership info failed:", error);
    });
  }
}

function getLoginId() {
  return cookies.getJSON("wapo_login_id");
}

function isLoggedIn() {
  return !!getLoginId();
}
/**
 * get the podcast url for a given podcast name
 *
 * @typedef Podcast
 * @type {Object}
 * @property {display_name}
 * @property {feed_url}
 *
 * @param {String} podcastName should equal the `display_name` of one entry from the second param
 * @param {Array<Podcast>} podcastsList personalized feed links from myslate
 * @return {String|undefined}
 */


function getFeedUrl(podcastName, podcastsList) {
  for (var i = 0; i < podcastsList.length; i++) {
    var p = podcastsList[i];

    if (p.display_name === podcastName) {
      return p.feed_url;
    }
  }
}

module.exports.getLoginId = getLoginId;
module.exports.getMembershipType = getMembershipType;
module.exports.getMembershipInfo = getMembershipInfo;
module.exports.isLoggedIn = isLoggedIn;
module.exports.isMember = isMember;
module.exports.getFeedUrl = getFeedUrl;
