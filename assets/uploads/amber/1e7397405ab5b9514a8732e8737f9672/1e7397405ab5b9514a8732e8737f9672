window.modules["28"] = [function(require,module,exports){"use strict";

// https://www.w3.org/TR/wai-aria-practices-1.1/#disclosure
// classnames match rulesets in _dropdown.scss
var dropdownSelector = ".dropdown";
var openModifier = "dropdown--open";
var buttonSelector = ".dropdown__button";
var contentSelector = ".dropdown__content";

module.exports = function (el) {
  var dropdownEl = el.matches(dropdownSelector) ? el : el.querySelector(dropdownSelector); // some views have conditional dropdowns, e.g. based on whether the user is logged in
  // check for that here rather than requiring each client script to do the same check

  if (!dropdownEl) return;
  var content = dropdownEl.querySelector(contentSelector);
  content.id = "dropdown-" + new Date().getTime(); // poor person's uid

  var button = dropdownEl.querySelector(buttonSelector);
  button.setAttribute("aria-controls", content.id);
  var closingEventListeners = {
    click: function click(e) {
      if (!dropdownEl.contains(e.target)) close();
    },
    keyup: function keyup(e) {
      if (["Escape", "Esc"].includes(e.key)) close();
    }
  }; // let listeners = 0;

  function open() {
    dropdownEl.classList.add(openModifier);
    button.setAttribute("aria-expanded", "true");

    for (var type in closingEventListeners) {
      document.body.addEventListener(type, closingEventListeners[type]);
    } // console.debug("listeners", ++listeners);

  }

  function close() {
    dropdownEl.classList.remove(openModifier);
    button.setAttribute("aria-expanded", "false");

    for (var type in closingEventListeners) {
      document.body.removeEventListener(type, closingEventListeners[type]);
    } // console.debug("listeners", --listeners); // should always settle at zero!

  }

  button.addEventListener("click", function () {
    return dropdownEl.classList.contains(openModifier) ? close() : open();
  });
};
}, {}];
window.modules["40"] = [function(require,module,exports){"use strict";

module.exports.track = function (eventName, extraData, options) {
  try {
    // check for permutive Data object
    var data = Object.assign({}, window.slatePermutiveData && window.slatePermutiveData.page || {});

    if (extraData) {
      data = Object.assign(data, extraData);
    }

    if (data.article && data.article.gsChannels) {
      delete data.article.gsChannels;
    }

    window.permutive && window.permutive.track(eventName, data, options);
  } catch (e) {
    console.error("failed to log permutive", e);
  }
};
}, {}];
window.modules["46"] = [function(require,module,exports){"use strict";

var abStringRe = new RegExp("^([0-9]+)_([0-9]+)$");

function parseAbTestString(abTestString) {
  var abTest = abStringRe.exec(abTestString);
  return abTest;
}

function isValidAbTestString(abTestString) {
  return !!parseAbTestString(abTestString);
}

module.exports.parseAbTestString = parseAbTestString;
module.exports.isValidAbTestString = isValidAbTestString;
