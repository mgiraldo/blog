window.modules["masthead.client"] = [function(require,module,exports){"use strict";

/* globals document: false, location: false, DS: false, URL: false */
// todo: should refactor this into a DS.service, especially if we include it anywhere else
var membership = require(3);

var dropdown = require(28);

DS.controller("masthead", [function () {
  var Constructor = function Constructor(el) {
    // highlight the appropriate section
    var sectionLinks = el.querySelectorAll(".sections__link");

    for (var i = 0; i < sectionLinks.length; ++i) {
      var sectionLink = sectionLinks[i];

      if (location.pathname.indexOf(sectionLink.getAttribute("href")) !== -1) {
        sectionLink.classList.add("sections__link--current");
      }
    } // display the appropriate account links


    var selector = membership.isLoggedIn() ? ".js-logged-in" : ".js-logged-out";
    var accountLinks = el.querySelectorAll(selector);

    for (var _i = 0; _i < accountLinks.length; _i++) {
      accountLinks[_i].hidden = false;
    }

    this.menu = el.querySelector(".menu");
    dropdown(el);
  };

  Constructor.prototype = {
    events: {
      ".js-toggle-menu click": "toggleMenu",
      ".js-redirect-return click": "addRedirectParam"
    },
    toggleMenu: function toggleMenu(e) {
      this.menu.classList.toggle("menu--open");
      document.documentElement.classList.toggle("menu-open", this.menu.classList.contains("menu--open"));
    },
    addRedirectParam: function addRedirectParam(e) {
      var url = new URL(e.currentTarget.href);
      url.searchParams.set("redirect_uri", location.href);
      e.currentTarget.href = url.toString();
    }
  };
  return Constructor;
}]);
