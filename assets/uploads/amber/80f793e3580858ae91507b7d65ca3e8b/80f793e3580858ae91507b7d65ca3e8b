<script type="text/javascript">
$(document).ready(function (e) {
	$("assets/61a32daa243336e589205e20abc561dbtn-toggle-search").on("click", function(e) {
		e.preventDefault();
		if ($('.searchSectionMobile').hasClass('d-none')) {
			$(".searchSectionMobile").removeClass('d-none');
			$('#mobile-menu').addClass('d-none');
		}
		else {
			$(".searchSectionMobile").addClass('d-none');
		}
		return false;
	});

	$('assets/61a32daa243336e589205e20abc561dbtn-toggle-menu').on('click', function (e) {
		e.preventDefault();
		if ($('#mobile-menu').hasClass('d-none')) {
			$('.searchSectionMobile').addClass('d-none');
			$('#mobile-menu').removeClass('d-none');
		}
		else {
			$('#mobile-menu').addClass('d-none');
		}
		return false;
	});

	$( "assets/61a32daa243336e589205e20abc561dbtn-close-mobile" ).on('click', function(e) {
		$('#mobile-menu').addClass('d-none');
		e.preventDefault();
	});
});
</script>
	


