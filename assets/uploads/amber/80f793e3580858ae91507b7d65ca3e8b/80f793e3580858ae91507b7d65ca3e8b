function initTaboolaWidget () {

	var taboolaCardRenderCount = 0;
	var infiniteScrollThreshold = HSW.utilities.isMobile() ? 9 : 5;
	function taboolaEventHandler (e) {
		if (e.type === 'api::render') {

			if (e.detail.mode === 'organic-thumbnails-rr-2') {
				dataLayer.push({
					'event'         : 'raw-event',
					'eventCategory' : 'taboola-organic',
					'eventAction'   : 'render',
					'eventLabel'    : e.detail.placement
				});
			}
			else {
				taboolaCardRenderCount++;

				if (['organic-thumbnails-feed-hero', 'organic-thumbnails-feed-carousel-readMore-mobile'].contains(e.detail.mode)) {
					dataLayer.push({
						'event'         : 'raw-event',
						'eventCategory' : 'taboola-feed',
						'eventAction'   : 'render',
						'eventLabel'    : e.detail.placement
					});
				}
				else if (taboolaCardRenderCount > infiniteScrollThreshold
					&& ['organic-thumbnails-feed-m', 'thumbnails-feed-s'].contains(e.detail.mode)) {
					dataLayer.push({
						'event'         : 'raw-event',
						'eventCategory' : 'taboola-feed',
						'eventAction'   : 'infinite-scroll-load',
						'eventLabel'    : e.detail.placement
					});
				}
			}

		}
		if (e.type === 'api::click') {
			if (e.detail.mode === 'organic-thumbnails-rr-2') {
				dataLayer.push({
					'event'         : 'raw-event',
					'eventCategory' : 'taboola-organic',
					'eventAction'   : 'click',
					'eventLabel'    : e.detail.placement
				});
			}
			else {
				dataLayer.push({
					'event'         : 'raw-event',
					'eventCategory' : 'taboola-feed',
					'eventAction'   : 'click',
					'eventLabel'    : e.detail.placement
				});
			}
		}
	}



	window._taboola = window._taboola || [];
	_taboola.push({ article:'auto' });
	_taboola.push({
		listenTo: 'render',
		handler: taboolaEventHandler
	});
	_taboola.push({
		listenTo: 'click',
		handler: taboolaEventHandler
	});

	!function (e, f, u, i) {
		if (!document.getElementById(i)){
			e.async = 1;
			e.src = u;
			e.id = i;
			f.parentNode.insertBefore(e, f);
		}
