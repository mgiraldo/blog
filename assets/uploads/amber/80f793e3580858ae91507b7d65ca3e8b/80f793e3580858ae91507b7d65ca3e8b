	<script>HSW.utilities.loadScript(HSW.assetUrl + 'js/ux.editorial.min.js', true);</script>
<script>
(function () {
	var options = {
		rootMargin: '0px',
		threshold: 0.5
	}
	var cb = function (pages, observer) {
		pages.forEach(function (page) {
			var logged    = page.target.dataset.logged;
			var pageUrl   = page.target.dataset.pageUrl;
			var pageTitle = page.target.textContent.trim();

			//When a new page comes into view and we haven't logged it yet, fire virtual PV to GA
			if (page.isIntersecting && logged === 'false') {

				dataLayer.push({
					"event"            : "virtual-page-view",
					"virtualPageUrl"   : pageUrl,
					"virtualPageTitle" : pageTitle
				});

				page.target.dataset.logged = 'true';
			}
		});
	};

	var observer = new IntersectionObserver(cb, options);

	var targets = document.querySelectorAll('.firePageview');
	targets.forEach(function (target) {
		observer.observe(target);
	});

	//Add listeners for collapsing/expanding pages
	$('.page-body').on('hide.bs.collapse', function () {
		var pageId = $(this).attr('id');

		dataLayer.push({
			'event' : 'raw-event-interactive',
			'eventCategory' : 'page-interaction',
			'eventAction'   : 'hide-page',
			'eventLabel'    : pageId
		});
	});
	$('.page-body').on('show.bs.collapse', function () {
		var pageId = $(this).attr('id');

		dataLayer.push({
			'event' : 'raw-event-interactive',
			'eventCategory' : 'page-interaction',
			'eventAction'   : 'show-page',
			'eventLabel'    : pageId
		});
	});
})();
</script>

