<script>
function loadBodyWidget () {
	if (HSW.utilities.hashArgs['recirc-widget'] === 'off') {
		$('#recirc-body').remove();
		return;
	}

	var assetType    = "Editorial Content";
	var templateName = "Article";

	window._taboola = window._taboola || [];
	// start with article settings for feed
	var mode      = 'thumbnails-e';
	var placement = 'Below Article Feed';
	var container = 'taboola-below-article-feed';
	var gaLabel   = 'taboola_below_article_feed';

	if (assetType == 'Landing Page') {
		placement = 'Below Landing Feed';
		container = 'taboola-below-main-column-feed';
		gaLabel   = 'taboola_below_homepage_feed';
		mode      = 'thumbnails-c';
		if (HSW.utilities.isMobile())
		{
			placement += ' - Mobile';
		}
	}
	else {
		// Read More on mobile (only active for organic traffic)
		if (  ['NewsPost', 'Article', 'Top10', 'Quiz', 'PhotoBlog', 'Recipe'].contains(templateName)
		   && (pageMetricsData.mkcpgn.length === 0 || pageMetricsData.mkcpgn === 'organic')
		   && HSW.utilities.isMobile()) {
			placement += ' - RM';
			mode       = 'organic-thumbnails-feed-carousel-readMore';
		}
	}

	placement += (templateName ? ' - ' + templateName : '') + (GACtest ? ' - ' + GACtest : '');

	_taboola.push({
		mode: mode,
		container: container,
		placement: placement,
		target_type: 'mix'
	});

	dataLayer.push({
		url         : location.pathname + location.hash,
		event       : "object-load",
		eventAction : "taboola-widget",
		eventLabel  : gaLabel
	});
};

if (GACtest === 'test73') {
	addToPQ(function () {
		//Lazy-load all of Taboola (including its main library) only when user scrolls
		//within 800px of the Feed widget's placement.
		HSW.utilities.lazyLoadCallback(800, document.querySelector('#tb_body_wrapper'), function () {
			initTaboolaWidget();
			loadBodyWidget();
			window._taboola = window._taboola || [];
			_taboola.push({flush: true});
		});
	});
}
else {
	addToPQ(loadBodyWidget);
}

$(document).ready(function (e) {
	if (pageMetricsData.template !== 'Quiz' || HSW.utilities.getViewportWidth() < 1200) {
		$('assets/61a32daa243336e589205e20abc561dbd-wrap-bannerBottom').remove();
	}
});

</script>
