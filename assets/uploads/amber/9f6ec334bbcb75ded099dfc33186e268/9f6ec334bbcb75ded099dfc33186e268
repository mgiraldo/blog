	<!-- FADE IN CODE -->
	
	<script type="text/javascript">
		function ShowBox($url)
		{
			//$url = "subhtml_golive/2000wings1.html";
			
			$(document).ready(function(){
				$("#lightbox").load($url);
			});
			
			$("#lightbox").fadeIn( 500 );
			$("#blackbox").fadeIn( 500 );
			$('body').css('overflow', 'hidden');
			$(window).resize();
		}
		
		function HideBox()
		{
			$("#lightbox").fadeOut( 500 );
			$("#blackbox").fadeOut( 500 );
			$('body').css('overflow', 'auto');
		}
		$(window).resize(function() {
			$('#lightbox').height($(window).height() * .75);
		});
    </script>



    
