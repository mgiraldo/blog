<script type="text/javascript">
window.addEventListener('load',translateContent,false);
var currentMeaTab = 'Alpha';
function translateContent() {
    var feature = document.getElementById('featureContent');
    var alphaBase = document.getElementById('featureMobile');
    var content = feature.innerHTML;
    alphaBase.innerHTML = content;
}
function selectMeaTab(tab) {
    document.getElementById('meaTab'+currentMeaTab).className = '';
    document.getElementById('meaIndex'+currentMeaTab).style.display = 'none';
    currentMeaTab = tab;
    document.getElementById('meaTab'+currentMeaTab).className = 'selectedTab';
    document.getElementById('meaIndex'+currentMeaTab).style.display = 'block';
}
</script>

