$(document).ready(function() {
    var timeout = null;
    var static_layers = $("#static_license_layers")[0];
    var dyne_layers = $("#license_layers")[0];

    if (Modernizr.csstransforms) {
	static_layers.style.display = "none";
	dyne_layers.style.display = "block";
    
